samtools faidx 22.fa 22:20000001-21000000 > 22_20_21M.fa

awk 'BEGIN {OFS="\t"} {if($3 >= 20000000 && $3 <= 21000000) {$2="22:20000001-21000000"; $3=$3-20000000; $4=$4-20000000; print}}' snp142Common_22.txt > snp142Common_22_20_21M.txt

../extract_snps.py --testset 22_20_21M.fa snp142Common_22_20_21M.txt > 22_20_21M.snp
